{
  "name": "svc/util-bundle",
  "description": "Helper package with some utilities (network, environment, mail, stimulus controllers)",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
      {
          "name": "Sven Vetter",
          "email": "git.09o@svenvetter.com"
      }
  ],
  "keywords": [
    "symfony-ux"
  ],
  "require": {
    "php": "^7.3.0 | ^8.0.0",
    "symfony/framework-bundle": "^5",
    "symfony/options-resolver": "^5",
    "symfony/mailer": "^5",
    "symfony/twig-bundle": "^5"
  },
  "autoload": {
    "psr-4": {
      "Svc\\UtilBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Svc\\UtilBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^5",
    "symfony/browser-kit": "^5",
    "phpstan/phpstan": "^1"
  },
  "scripts": {
    "test": "vendor/bin/simple-phpunit",
    "phpstan": "php -d memory_limit=-1 vendor/bin/phpstan analyse src/ --level 5 -c .phpstan.neon"
  }
}
