{
  "name": "svc/util-bundle",
  "description": "Helper package with some utilities (network, environment, mail)",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
      {
          "name": "Sven Vetter",
          "email": "git.09o@svenvetter.com"
      }
  ],
  "require": {
    "php": "^7.3.0 | ^8.0.0",
    "symfony/framework-bundle": "^5",
    "symfony/options-resolver": "^5",
    "symfony/mailer": "^5",
    "twig/twig": "^2.12|^3.0",
    "symfony/twig-bundle": "^4.3|^5.0"
  },
  "autoload": {
    "psr-4": {
      "Svc\\UtilBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Svc\\UtilBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^5",
    "symfony/browser-kit": "^5"
  },
  "scripts": {
    "test": "vendor/bin/simple-phpunit"
  }
}
