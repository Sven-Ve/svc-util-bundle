{
  "name": "@svc/util-bundle",
  "description": "general stimulus controllers for svc bundles",
  "license": "MIT",
  "version": "2.5.0",
  "keywords": [
    "symfony-ux"
  ],
  "repository": "https://github.com/Sven-Ve/svc-util-bundle",
  "symfony": {
    "controllers": {
      "clipboard": {
        "main": "src/clipboard.js",
        "webpackMode": "eager",
        "fetch": "lazy",
        "enabled": true
      },
      "mclipboard": {
        "main": "src/mclipboard.js",
        "fetch": "lazy",
        "enabled": true
      },
      "alert": {
        "main": "src/alert.js",
        "fetch": "lazy",
        "enabled": true
      },
      "submit-confirm": {
        "main": "src/submit-confirm.js",
        "fetch": "lazy",
        "enabled": true
      },
      "wysiwyg": {
        "main": "src/wysiwyg.js",
        "fetch": "lazy",
        "enabled": true
      },
      "show-password": {
        "main": "src/show-password.js",
        "fetch": "lazy",
        "enabled": true
      },
      "reload-content": {
        "main": "src/reload-content.js",
        "fetch": "lazy",
        "enabled": true
      },
      "modal": {
        "main": "src/modal.js",
        "fetch": "lazy",
        "enabled": true
      },
      "toast": {
        "main": "src/toast_controller.js",
        "fetch": "lazy",
        "enabled": true
      },
      "autosubmit": {
        "main": "src/autosubmit_controller.js",
        "fetch": "lazy",
        "enabled": true
      },
      "toggle-password": {
        "main": "src/toggle_password.js",
        "fetch": "eager",
        "enabled": true,
        "autoimport": {
          "@svc/util-bundle/styles/toggle_password.css": true
        }
      }
    },
    "importmap": {
      "@hotwired/stimulus": "^3.2.0",
      "sweetalert2": "*",
      "@ckeditor/ckeditor5-build-classic": "*",
      "debounce": "*"
    }
  },
  "peerDependencies": {
    "sweetalert2": ">=11",
    "@hotwired/stimulus": "^3.0.0",
    "@ckeditor/ckeditor5-build-classic": ">=30"
  },
  "devDependencies": {
    "sweetalert2": ">=11",
    "@hotwired/stimulus": "^3.0.0",
    "@ckeditor/ckeditor5-build-classic": ">=30"
  }
}