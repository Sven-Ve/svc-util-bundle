<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
      <service id="svc_util.service.networkhelper" class="Svc\UtilBundle\Service\NetworkHelper" public="true">
      </service>
      <service id="Svc\UtilBundle\Service\NetworkHelper" alias="svc_util.service.networkhelper" />

      <service id="svc_util.service.env-info-helper" class="Svc\UtilBundle\Service\EnvInfoHelper" public="true">
      </service>
      <service id="Svc\UtilBundle\Service\EnvInfoHelper" alias="svc_util.service.env-info-helper" />

      <service id="svc_util.service.mailhelper" class="Svc\UtilBundle\Service\MailerHelper" public="true" autowire="true">
        <tag name="controller.service_arguments"/>
      </service>
      <service id="Svc\UtilBundle\Service\MailerHelper" alias="svc_util.service.mailhelper" />

      <service id="svc_util.controller.envinfo" class="Svc\UtilBundle\Controller\EnvInfoController" public="true" autowire="true">
        <call method="setContainer">
            <argument type="service" id="Psr\Container\ContainerInterface"/>
        </call>
        <tag name="container.service_subscriber"/>
        <tag name="controller.service_arguments"/>
      </service>

      <service id="svc_util.controller.contact" class="Svc\UtilBundle\Controller\ContactController" public="true" autowire="true">
        <call method="setContainer">
            <argument type="service" id="Psr\Container\ContainerInterface"/>
        </call>
        <tag name="container.service_subscriber"/>
        <tag name="controller.service_arguments"/>
      </service>


    </services>
</container>